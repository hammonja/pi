#log /dev/null
device /dev/video0
skip 10
jpeg 100
#deinterlace
resolution 1600x1200
no-banner
set "Sharpness"=200
#set "White Balance Temperature, Auto"=False
set "White Balance Temperature, Auto"=True
set "Backlight Compensation"=0
set "Brightness"=160
set "Contrast"=100
#32 is default
set "Saturation"=28
#28 is default
frames 1
#frames 255
#loop 1
save /var/www/output.jpg
